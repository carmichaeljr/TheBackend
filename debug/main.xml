<CompileCmd cmd="g++">
	<Arg addSpace=true>-static-libgcc</Arg>
	<Arg addSpace=true>-static-libstdc++</Arg>
	<Arg addSpace=true>-I .</Arg>
	<Arg addSpace=true>-Wall -o</Arg>
	<Arg>"bin/mainDebug"</Arg>
	<Arg addSpace=true>debug/*.cpp</Arg>
	<Arg addSpace=true>src/*.cpp</Arg>
</CompileCmd>
<ExecuteCmd>
	<Arg>"./bin/mainDebug"</Arg>
</ExecuteCmd>
<LineCount>
	<Files>debug/*.hpp</Files>
	<Files>debug/*.cpp</Files>
	<Files>debug/*.tpp</Files>
	<Files>src/*.hpp</Files>
	<Files>src/*.cpp</Files>
	<Files>src/*.tpp</Files>
</LineCount>
